{{#*inline "single_content"}}
A list of my active accounts.
{{#load_file "social.json"}}
    <div class="card-container mdl-grid">
        {{#each this}}
        <a href="{{url}}" rel="noopener" class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--4-col link-card">
            <div class="image-wrapper" data-background="#2d3a4a">
                <div class="mdl-card__title">
                    <i id="social_i_{{toId name}}" class="card-icon icon-{{icon}}" aria-hidden="true" title="{{username}} on {{name}}" aria-label="{{username}} on {{name}}"></i>
                </div>
            </div>
            <div class="mdl-card__supporting-text">{{username}} on {{name}}</div>
            </a>
        {{/each}}
    </div>
{{/load_file}}
{{/inline}}
{{> layout/single
title_short = "Profiles"
title="Profiles | James Tapsell"
description="A list of my social profiles"
url="https://www.jrtapsell.co.uk/profiles.html"}}
