{{#*inline "ld_json"}}
<script type="application/ld+json">
{
"@context" : "http://schema.org",
"@type" : "Person",
"name" : "James Tapsell",
"url" : "https://www.jrtapsell.co.uk",
"sameAs" : [
{{#load_file "social.json"}}
    {{#each this}}
        "{{url}}"{{#unless @last}},{{/unless}}
    {{/each}}
{{/load_file}}
],
"email": "contact@jrtapsell.co.uk",
"image": "https://www.jrtapsell.co.uk/img/icon512.jpg"
}
</script>
{{/inline}}
{{#*inline "content"}}
<div id="about">
    <h2>About Me</h2>
    <div class="mdl-grid">
        <div class="mdl-cell mdl-cell--2-col"></div>
        <div class=" mdl-cell mdl-cell--8-col">
            <p>
                My name is James Tapsell, I am student at <b>Royal Holloway, University of London</b>, studying <b>MSci Computer
                Science (Information Security) with a year in industry.</b>
            </p>
            <p>
                I take part in CTFs with the TU6PM team, this allows me to learn new skills and by seeing
                flaws in systems and exploiting them I aim to make systems I develop more secure.
            </p>
            <p>
                I use the username <kbd class="inline">jrtapsell</kbd> on Github, Telegram, Keybase, LinkedIn, Facebook and Twitter.
            </p>
        </div>
    </div>
</div>
{{#load_file "data.json"}}
<div id="target">
    {{#each this}}
        <h2 id="{{name}}">{{name}}</h2>
        {{#with children}}
            <div class="card-container mdl-grid">
                {{#each this}}
                    <a {{#if url}}href="{{url}}"{{/if}} rel="noopener" class="mdl-card mdl-shadow--2dp mdl-cell mdl-cell--4-col link-card">
                        <div class="image-wrapper" data-background="{{#if backing}}{{backing}}{{else}}#2d3a4a{{/if}}">
                            <div class="mdl-card__title" {{#if icon}}data-background="url('{{{icon}}}') center center / contain no-repeat"{{/if}}>
                                <h3 class="mdl-card__title-text card-title">{{name}}</h3>
                            </div>
                        </div>
                        <div class="mdl-card__supporting-text">{{description}}</div>
                    {{#if url}}</a>{{/if}}
                {{/each}}
            </div>
        {{/with}}
    {{/each}}
</div>
{{/load_file}}
{{/inline}}
{{> layout/base
    title="James Tapsell"
    description="The personal site of James Tapsell, third year Computer Science with Information Security student at Royal Holloway, University of London"
    url="https://www.jrtapsell.co.uk"}}
